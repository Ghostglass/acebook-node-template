<h1>Welcome {{username}}</h1>
<form action="/dashboard/new" method="get">
  <input type="submit" class="btn btn-primary" value="Add new post">
</form>


<ul class="content">
  {{#each content}}
    <div class="card text-white bg-primary mb-3" style="max-width: 20rem;">
    <div class="card-header">{{this.user.username}} </div>
    <div class="card-body">
      <h4 class="card-title">{{this.post}}</h4>
      <p class="card-text">{{this.createdAt}}</p>
    </div>
  </div>
  {{/each}}
</ul>





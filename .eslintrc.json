{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true,
        "jest/globals": true,
        "cypress/globals": true
    }
  }
}
  {
  "extends": "eslint:recommended"
  "plugin:node/recommended": true, 
  },
  {
    "globals": 
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "plugins": [
      "cypress",
      "jest",
      "node"
    ],
    "rules": {
    }
  
  {
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    },
  },
  "env": {
    "browser": true
  },
  "extends": ["airbnb", "prettier"],
  "rules": {
    "react/prop-types": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".js"] }]
  },
}

